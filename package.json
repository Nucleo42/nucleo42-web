{
  "name": "nucleo42",
  "version": "0.0.0",
  "scripts": {
    "preinstall": "git config core.hooksPath hooks && chmod +x hooks/*",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "format": "prettier --write",
    "format:project": "prettier --write \"**/*\"",
    "lint": "eslint .",
    "lint:fix": "npm run lint --fix",
    "test": "jest --passWithNoTests --no-cache --runInBand --silent --noStackTrace",
    "test:related": "jest --watch --onlyChanged",
    "test:unit": "npm test -- --watch -c jest-unit-config.js",
    "test:integration": "npm test -- --watch -c jest-integration-config.js",
    "test:unit:watch": "npm test -- --watchAll -c jest-unit-config.js",
    "test:integration:watch": "npm test -- --watchAll -c jest-integration-config.js",
    "test:unit:verbose": "npm test -- --verbose -c jest-unit-config.js",
    "test:integration:verbose": "npm test -- --verbose -c jest-integration-config.js",
    "test:staged": "npm test --findRelatedTests",
    "test:ci": "jest --passWithNoTests --coverage"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~19.0.3",
    "@angular/cdk": "~19.1.2",
    "@angular/common": "~19.0.3",
    "@angular/compiler": "~19.0.3",
    "@angular/core": "~19.0.3",
    "@angular/forms": "~19.0.3",
    "@angular/material": "~19.1.2",
    "@angular/platform-browser": "~19.0.3",
    "@angular/platform-browser-dynamic": "~19.0.3",
    "@angular/router": "~19.0.3",
    "rxjs": "~7.8.1",
    "tslib": "~2.8.1",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-builders/jest": "~19.0.0",
    "@angular-devkit/build-angular": "~19.0.4",
    "@angular/cli": "~19.0.4",
    "@angular/compiler-cli": "~19.0.3",
    "@commitlint/cli": "~19.6.1",
    "@commitlint/config-conventional": "~19.6.0",
    "@eslint/js": "~9.17.0",
    "@types/jest": "~29.5.14",
    "@typescript-eslint/eslint-plugin": "~8.18.2",
    "eslint-config-prettier": "~9.1.0",
    "eslint-plugin-prettier": "~5.2.1",
    "jest": "~29.7.0",
    "jest-preset-angular": "~14.5.1",
    "lint-staged": "~15.4.1",
    "prettier": "~3.4.2",
    "ts-jest": "~29.2.5",
    "typescript": "~5.6.3",
    "typescript-eslint": "~8.18.2"
  }
}

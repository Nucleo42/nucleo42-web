{
  "name": "nucleo42",
  "version": "0.0.0",
  "scripts": {
    "preinstall": "git config core.hooksPath hooks",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "format": "prettier --write",
    "format:project": "prettier --write \"**/*\"",
    "lint": "eslint .",
    "lint:fix": "npm run lint --fix",
    "test": "jest",
    "test:related": "jest --watch --onlyChanged",
    "test:unit": "jest --selectProjects 'Unit Tests'",
    "test:integration": "jest --selectProjects 'Integration Tests'",
    "test:unit:watch": "jest --selectProjects 'Unit Tests' --watch",
    "test:integration:watch": "jest --selectProjects 'Integration Tests' --watch",
    "test:unit:verbose": "jest --selectProjects 'Unit Tests' --verbose",
    "test:integration:verbose": "jest --selectProjects 'Integration Tests' --verbose"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~19.0.3",
    "@angular/common": "~19.0.3",
    "@angular/compiler": "~19.0.3",
    "@angular/core": "~19.0.3",
    "@angular/forms": "~19.0.3",
    "@angular/platform-browser": "~19.0.3",
    "@angular/platform-browser-dynamic": "~19.0.3",
    "@angular/router": "~19.0.3",
    "rxjs": "~7.8.1",
    "tslib": "~2.8.1",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-builders/jest": "^19.0.0",
    "@angular-devkit/build-angular": "~19.0.4",
    "@angular/cli": "~19.0.4",
    "@angular/compiler-cli": "~19.0.3",
    "@commitlint/cli": "~19.6.1",
    "@commitlint/config-conventional": "~19.6.0",
    "@eslint/js": "~9.17.0",
    "@types/jest": "~29.5.14",
    "@typescript-eslint/eslint-plugin": "~8.18.2",
    "eslint-config-prettier": "~9.1.0",
    "eslint-plugin-prettier": "~5.2.1",
    "jest": "~29.7.0",
    "jest-preset-angular": "~14.5.1",
    "lint-staged": "~15.4.1",
    "prettier": "~3.4.2",
    "ts-jest": "^29.2.5",
    "typescript": "~5.6.3",
    "typescript-eslint": "~8.18.2"
  }
}
